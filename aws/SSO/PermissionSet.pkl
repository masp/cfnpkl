/// Resource Type definition for SSO PermissionSet
extends ".../AWS/Resource.pkl"
Type = "AWS::SSO::PermissionSet"
Properties: Props?
class Props {
    /// The name you want to assign to this permission set.
    Name: String(length <= 32 && length >= 1 && matches(Regex(#"[\w+=,.@-]+"#)))?
    /// The permission set that the policy will be attached to
    PermissionSetArn: String(length <= 1224 && length >= 10 && matches(Regex(#"arn:(aws|aws-us-gov|aws-cn|aws-iso|aws-iso-b):sso:::permissionSet/(sso)?ins-[a-zA-Z0-9-.]{16}/ps-[a-zA-Z0-9-./]{16}"#)))?
    /// The permission set description.
    Description: String(length <= 700 && length >= 1 && matches(Regex(#"[\u0009\u000A\u000D\u0020-\u007E\u00A1-\u00FF]*"#)))?
    /// The sso instance arn that the permission set is owned.
    InstanceArn: String(length <= 1224 && length >= 10 && matches(Regex(#"arn:(aws|aws-us-gov|aws-cn|aws-iso|aws-iso-b):sso:::instance/(sso)?ins-[a-zA-Z0-9-.]{16}"#)))?
    /// The length of time that a user can be signed in to an AWS account.
    SessionDuration: String(length <= 100 && length >= 1 && matches(Regex(#"^(-?)P(?=\d|T\d)(?:(\d+)Y)?(?:(\d+)M)?(?:(\d+)([DW]))?(?:T(?:(\d+)H)?(?:(\d+)M)?(?:(\d+(?:\.\d+)?)S)?)?$"#)))?
    /// The relay state URL that redirect links to any service in the AWS
    /// Management Console.
    RelayStateType: String(length <= 240 && length >= 1 && matches(Regex(#"[a-zA-Z0-9&amp;$@#\/%?=~\-_'&quot;|!:,.;*+\[\]\ \(\)\{\}]+"#)))?
    ManagedPolicies: Listing<String(length <= 2048 && length >= 20)>?
    /// The inline policy to put in permission set.
    InlinePolicy: Mapping | String | *Null
    Tags: Listing<Tag>?
    CustomerManagedPolicyReferences: Listing<CustomerManagedPolicyReference>?
    PermissionsBoundary: PermissionsBoundary?
}
/// The metadata that you apply to the permission set to help you
/// categorize and organize them.
class Tag {
    Key: String(length <= 128 && length >= 1 && matches(Regex(#"[\w+=,.@-]+"#)))
    Value: String(length <= 256 && length >= 0 && matches(Regex(#"[\w+=,.@-]+"#)))
}
class CustomerManagedPolicyReference {
    Name: String(length <= 128 && length >= 1 && matches(Regex(#"[\w+=,.@-]+"#)))
    Path: String(length <= 512 && length >= 1 && matches(Regex(#"((/[A-Za-z0-9\.,\+@=_-]+)*)/"#)))?
}
class PermissionsBoundary {
    CustomerManagedPolicyReference: CustomerManagedPolicyReference?
    ManagedPolicyArn: String(length <= 2048 && length >= 20)?
}
