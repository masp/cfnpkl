/// Resource Type definition for AWS::MSK::ClusterPolicy
extends ".../AWS/Resource.pkl"
Type = "AWS::MSK::ClusterPolicy"
Properties: Props?
class Props {
    /// A policy document containing permissions to add to the specified
    /// cluster.
    Policy: Mapping?
    /// The arn of the cluster for the resource policy.
    ClusterArn: String(matches(Regex(#"^arn:[\w-]+:kafka:[\w-]+:\d+:cluster.*\Z"#)))?
    /// The current version of the policy attached to the specified cluster
    CurrentVersion: String(matches(Regex(#"^(K)([a-zA-Z0-9]+)\Z"#)))?
}
