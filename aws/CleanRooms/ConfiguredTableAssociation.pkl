/// Represents a table that can be queried within a collaboration
extends ".../AWS/Resource.pkl"
Type = "AWS::CleanRooms::ConfiguredTableAssociation"
Properties: Props?
class Props {
    Arn: String(length <= 100)?
    /// An arbitrary set of tags (key-value pairs) for this cleanrooms
    /// collaboration.
    Tags: Listing<Tag>?
    ConfiguredTableAssociationIdentifier: String(length <= 36 && length >= 36 && matches(Regex(#"[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"#)))?
    ConfiguredTableIdentifier: String(length <= 36 && length >= 36 && matches(Regex(#"[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"#)))?
    Description: String(length <= 255 && matches(Regex(#"^[\u0020-\uD7FF\uE000-\uFFFD\uD800\uDBFF-\uDC00\uDFFF\t\r\n]*$"#)))?
    MembershipIdentifier: String(length <= 36 && length >= 36 && matches(Regex(#"[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"#)))?
    Name: String(length <= 128 && matches(Regex(#"^[a-zA-Z0-9_](([a-zA-Z0-9_ ]+-)*([a-zA-Z0-9_ ]+))?$"#)))?
    RoleArn: String(length <= 512 && length >= 32)?
}
class Tag {
    Key: String(length <= 128 && length >= 1)
    Value: String(length <= 256 && length >= 1)
}
