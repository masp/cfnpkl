/// Resource Type definition for AWS::Config::StoredQuery
extends ".../AWS/Resource.pkl"
Type = "AWS::Config::StoredQuery"
Properties: Props?
class Props {
    QueryArn: String(length <= 500 && length >= 1)?
    QueryId: String(length <= 36 && length >= 1 && matches(Regex(#"^\S+$"#)))?
    QueryName: String(length <= 64 && length >= 1 && matches(Regex(#"^[a-zA-Z0-9-_]+$"#)))?
    QueryDescription: String(length <= 256 && length >= 0 && matches(Regex(#"[\s\S]*"#)))?
    QueryExpression: String(length <= 4096 && length >= 1 && matches(Regex(#"[\s\S]*"#)))?
    /// The tags for the stored query.
    Tags: Listing<Tag>?
}
/// A key-value pair to associate with a resource.
class Tag {
    /// The key name of the tag. You can specify a value that is 1 to 127
    /// Unicode characters in length and cannot be prefixed with aws:. You can
    /// use any of the following characters: the set of Unicode letters,
    /// digits, whitespace, _, ., /, =, +, and -.
    Key: String(length <= 128 && length >= 1)
    /// The value for the tag. You can specify a value that is 0 to 255
    /// Unicode characters in length and cannot be prefixed with aws:. You can
    /// use any of the following characters: the set of Unicode letters,
    /// digits, whitespace, _, ., /, =, +, and -.
    Value: String(length <= 256 && length >= 0)
}
