/// Resource schema for AWS::DMS::DataProvider
extends ".../AWS/Resource.pkl"
Type = "AWS::DMS::DataProvider"
Properties: Props?
class Props {
    /// The property describes a name to identify the data provider.
    DataProviderName: String(length <= 255 && length >= 1)?
    /// The property describes an identifier for the data provider. It is used
    /// for describing/deleting/modifying can be name/arn
    DataProviderIdentifier: String(length <= 255 && length >= 1)?
    /// The data provider ARN.
    DataProviderArn: String(length <= 255 && length >= 1)?
    /// The data provider creation time.
    DataProviderCreationTime: String(length <= 40 && length >= 1)?
    /// The optional description of the data provider.
    Description: String(length <= 255 && length >= 1)?
    /// The property describes a data engine for the data provider.
    Engine: "postgresql" | "mysql" | "oracle" | "sqlserver" | "aurora" | "aurora_postgresql" | *Null
    /// The property describes the exact settings which can be modified
    ExactSettings: Boolean?
    /// The property identifies the exact type of settings for the data
    /// provider.
    Settings: Settings0 | Settings1 | Settings2 | Settings3 | *Null
    /// An array of key-value pairs to apply to this resource.
    Tags: Listing<Tag>?
}
