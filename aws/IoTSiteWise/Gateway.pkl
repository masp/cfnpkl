/// Resource schema for AWS::IoTSiteWise::Gateway
extends ".../AWS/Resource.pkl"
Type = "AWS::IoTSiteWise::Gateway"
Properties: Props?
class Props {
    /// A unique, friendly name for the gateway.
    GatewayName: String?
    /// The gateway's platform. You can only specify one platform in a
    /// gateway.
    GatewayPlatform: Mapping | Mapping | *Null
    /// A list of key-value pairs that contain metadata for the gateway.
    Tags: Listing<Tag>?
    /// The ID of the gateway device.
    GatewayId: String?
    /// A list of gateway capability summaries that each contain a namespace
    /// and status.
    GatewayCapabilitySummaries: Listing<GatewayCapabilitySummary>?
}
/// To add or update tag, provide both key and value. To delete tag,
/// provide only tag key to be deleted
class Tag {
    Key: String
    Value: String
}
/// Contains a summary of a gateway capability configuration.
class GatewayCapabilitySummary {
    CapabilityNamespace: String
    CapabilityConfiguration: String?
}
