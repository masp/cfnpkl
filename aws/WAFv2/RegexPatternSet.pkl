/// Contains a list of Regular expressions based on the provided inputs.
/// RegexPatternSet can be used with other WAF entities with
/// RegexPatternSetReferenceStatement to perform other actions .
extends ".../AWS/Resource.pkl"
Type = "AWS::WAFv2::RegexPatternSet"
Properties: Props?
class Props {
    /// ARN of the WAF entity.
    Arn: String?
    /// Description of the entity.
    Description: String(matches(Regex(#"^[a-zA-Z0-9=:#@/\-,.][a-zA-Z0-9+=:#@/\-,.\s]+[a-zA-Z0-9+=:#@/\-,.]{1,256}$"#)))?
    /// Name of the RegexPatternSet.
    Name: String(matches(Regex(#"^[0-9A-Za-z_-]{1,128}$"#)))?
    /// Id of the RegexPatternSet
    Id: String(matches(Regex(#"^[0-9a-f]{8}-(?:[0-9a-f]{4}-){3}[0-9a-f]{12}$"#)))?
    RegularExpressionList: Listing<String>?
    /// Use CLOUDFRONT for CloudFront RegexPatternSet, use REGIONAL for
    /// Application Load Balancer and API Gateway.
    Scope: "CLOUDFRONT" | "REGIONAL" | *Null
    Tags: Listing<Tag>?
}
class Tag {
    Key: String(length <= 128 && length >= 1)?
    Value: String(length <= 256 && length >= 0)?
}
