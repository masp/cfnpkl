/// Resource Type Definition for AWS::S3Outposts::Endpoint
extends ".../AWS/Resource.pkl"
Type = "AWS::S3Outposts::Endpoint"
Properties: Props?
class Props {
    /// The Amazon Resource Name (ARN) of the endpoint.
    Arn: String(length <= 500 && length >= 5 && matches(Regex(#"^arn:[^:]+:s3-outposts:[a-zA-Z0-9\-]+:\d{12}:outpost\/[^:]+\/endpoint/[a-zA-Z0-9]{19}$"#)))?
    /// The VPC CIDR committed by this endpoint.
    CidrBlock: String(length <= 20 && length >= 1)?
    /// The time the endpoint was created.
    CreationTime: String(matches(Regex(#"^([0-2]\d{3})-(0[0-9]|1[0-2])-([0-2]\d|3[01])T([01]\d|2[0-4]):([0-5]\d):([0-6]\d)((\.\d{3})?)Z$"#)))?
    /// The ID of the endpoint.
    Id: String(length <= 500 && length >= 5 && matches(Regex(#"^[a-zA-Z0-9]{19}$"#)))?
    /// The network interfaces of the endpoint.
    NetworkInterfaces: Listing<NetworkInterface>?
    /// The id of the customer outpost on which the bucket resides.
    OutpostId: String(matches(Regex(#"^(op-[a-f0-9]{17}|\d{12}|ec2)$"#)))?
    /// The ID of the security group to use with the endpoint.
    SecurityGroupId: String(length <= 100 && length >= 1 && matches(Regex(#"^sg-([0-9a-f]{8}|[0-9a-f]{17})$"#)))?
    Status: "Available" | "Pending" | "Deleting" | "Create_Failed" | "Delete_Failed" | *Null
    /// The ID of the subnet in the selected VPC. The subnet must belong to
    /// the Outpost.
    SubnetId: String(length <= 100 && length >= 1 && matches(Regex(#"^subnet-([0-9a-f]{8}|[0-9a-f]{17})$"#)))?
    /// The type of access for the on-premise network connectivity for the
    /// Outpost endpoint. To access endpoint from an on-premises network, you
    /// must specify the access type and provide the customer owned Ipv4 pool.
    AccessType: "CustomerOwnedIp" | "Private" | *Null
    /// The ID of the customer-owned IPv4 pool for the Endpoint. IP addresses
    /// will be allocated from this pool for the endpoint.
    CustomerOwnedIpv4Pool: String(matches(Regex(#"^ipv4pool-coip-([0-9a-f]{17})$"#)))?
    /// The failure reason, if any, for a create or delete endpoint operation.
    FailedReason: FailedReason?
}
/// The container for the network interface.
class NetworkInterface {
    NetworkInterfaceId: String(length <= 100 && length >= 1)
}
class FailedReason {
    /// The failure code, if any, for a create or delete endpoint operation.
    ErrorCode: String?
    /// Additional error details describing the endpoint failure and
    /// recommended action.
    Message: String?
}
