/// Resource Type definition for
/// AWS::CodeStarNotifications::NotificationRule
extends ".../AWS/Resource.pkl"
Type = "AWS::CodeStarNotifications::NotificationRule"
Properties: Props?
class Props {
    EventTypeId: String(length <= 2048 && length >= 1)?
    CreatedBy: String(length <= 2048 && length >= 1)?
    TargetAddress: String(length <= 2048 && length >= 1)?
    EventTypeIds: Listing<String(length <= 200 && length >= 1)>?
    Status: "ENABLED" | "DISABLED" | *Null
    DetailType: "BASIC" | "FULL" | *Null
    Resource: String(matches(Regex(#"^arn:aws[^:\s]*:[^:\s]*:[^:\s]*:[0-9]{12}:[^\s]+$"#)))?
    Targets: Listing<Target>?
    Tags: Mapping?
    Name: String(length <= 64 && length >= 1 && matches(Regex(#"[A-Za-z0-9\-_ ]+$"#)))?
    Arn: String(matches(Regex(#"^arn:aws[^:\s]*:codestar-notifications:[^:\s]+:\d{12}:notificationrule\/(.*\S)?$"#)))?
}
class Target {
    TargetType: String
    TargetAddress: String
}
