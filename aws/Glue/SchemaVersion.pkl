/// This resource represents an individual schema version of a schema
/// defined in Glue Schema Registry.
extends ".../AWS/Resource.pkl"
Type = "AWS::Glue::SchemaVersion"
Properties: Props?
class Props {
    Schema: Schema?
    /// Complete definition of the schema in plain-text.
    SchemaDefinition: String(length <= 170000 && length >= 1)?
    /// Represents the version ID associated with the schema version.
    VersionId: String(matches(Regex(#"[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"#)))?
}
/// Identifier for the schema where the schema version will be created.
class Schema {
    /// Amazon Resource Name for the Schema. This attribute can be used to
    /// uniquely represent the Schema.
    SchemaArn: String(matches(Regex(#"arn:(aws|aws-us-gov|aws-cn):glue:.*"#)))?
    /// Name of the schema. This parameter requires RegistryName to be
    /// provided.
    SchemaName: String(length <= 255 && length >= 1)?
    /// Name of the registry to identify where the Schema is located.
    RegistryName: String(length <= 255 && length >= 1)?
}
