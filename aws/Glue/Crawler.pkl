    EventQueueArn: String?
}
class CatalogTarget {
    ConnectionName: String?
    DatabaseName: String?
    DlqEventQueueArn: String?
    Tables: Listing<String>?
    EventQueueArn: String?
}
class DeltaTarget {
    ConnectionName: String?
    CreateNativeDeltaTable: Boolean?
    WriteManifest: Boolean?
