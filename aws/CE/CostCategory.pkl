/// Cost Category enables you to map your cost and usage into meaningful
/// categories. You can use Cost Category to organize your costs using a
/// rule-based engine.
extends ".../AWS/Resource.pkl"
Type = "AWS::CE::CostCategory"
Properties: Props?
class Props {
    /// Cost category ARN
    Arn: String(matches(Regex(#"^arn:aws[-a-z0-9]*:[a-z0-9]+:[-a-z0-9]*:[0-9]{12}:[-a-zA-Z0-9/:_]+$"#)))?
    EffectiveStart: String(length <= 25 && length >= 20 && matches(Regex(#"^\d{4}-\d\d-\d\dT\d\d:\d\d:\d\d(([+-]\d\d:\d\d)|Z)$"#)))?
    Name: String(length <= 50 && length >= 1)?
    RuleVersion: "CostCategoryExpression.v1" | *Null
    /// JSON array format of Expression in Billing and Cost Management API
    Rules: String?
    /// Json array format of CostCategorySplitChargeRule in Billing and Cost
    /// Management API
    SplitChargeRules: String?
    /// The default value for the cost category
    DefaultValue: String(length <= 50 && length >= 1)?
}
