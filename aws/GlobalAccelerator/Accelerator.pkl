/// Resource Type definition for AWS::GlobalAccelerator::Accelerator
extends ".../AWS/Resource.pkl"
Type = "AWS::GlobalAccelerator::Accelerator"
Properties: Props?
class Props {
    /// Name of accelerator.
    Name: String(length <= 64 && length >= 1 && matches(Regex(#"^[a-zA-Z0-9_-]{0,64}$"#)))?
    /// IP Address type.
    IpAddressType: "IPV4" | "DUAL_STACK" | *Null
    /// The IP addresses from BYOIP Prefix pool.
    IpAddresses: Listing<String(matches(Regex(#"^(?:[0-9]{1,3}\.){3}[0-9]{1,3}$"#)))>?
    /// Indicates whether an accelerator is enabled. The value is true or
    /// false.
    Enabled: Boolean?
    /// The Domain Name System (DNS) name that Global Accelerator creates that
    /// points to your accelerator's static IPv4 addresses.
    DnsName: String?
    /// The IPv4 addresses assigned to the accelerator.
    Ipv4Addresses: Listing<String>?
    /// The IPv6 addresses assigned if the accelerator is dualstack
    Ipv6Addresses: Listing<String>?
    /// The Domain Name System (DNS) name that Global Accelerator creates that
    /// points to your accelerator's static IPv4 and IPv6 addresses.
    DualStackDnsName: String?
    /// The Amazon Resource Name (ARN) of the accelerator.
    AcceleratorArn: String?
    Tags: Listing<Tag>?
}
/// Tag is a key-value pair associated with accelerator.
class Tag {
    /// Key of the tag. Value can be 1 to 127 characters.
    Key: String(length <= 127 && length >= 1)
    /// Value for the tag. Value can be 1 to 255 characters.
    Value: String(length <= 255 && length >= 1)
}
