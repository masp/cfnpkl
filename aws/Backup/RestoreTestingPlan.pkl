/// Definition of AWS::Backup::RestoreTestingPlan Resource Type
extends ".../AWS/Resource.pkl"
Type = "AWS::Backup::RestoreTestingPlan"
Properties: Props?
class Props {
    RecoveryPointSelection: RestoreTestingRecoveryPointSelection?
    RestoreTestingPlanArn: String?
    RestoreTestingPlanName: String?
    ScheduleExpression: String?
    ScheduleExpressionTimezone: String?
    StartWindowHours: Int?
    Tags: Listing<Tag>?
}
class RestoreTestingRecoveryPointSelection {
    Algorithm: "LATEST_WITHIN_WINDOW" | "RANDOM_WITHIN_WINDOW"
    SelectionWindowDays: Int?
    RecoveryPointTypes: Listing<"SNAPSHOT" | "CONTINUOUS">
    IncludeVaults: Listing<String>
    ExcludeVaults: Listing<String>?
}
class Tag {
