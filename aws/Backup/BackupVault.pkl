/// Resource Type definition for AWS::Backup::BackupVault
extends ".../AWS/Resource.pkl"
Type = "AWS::Backup::BackupVault"
Properties: Props?
class Props {
    AccessPolicy: Mapping | String | *Null
    BackupVaultName: String(matches(Regex(#"^[a-zA-Z0-9\-\_]{2,50}$"#)))?
    BackupVaultTags: Mapping?
    EncryptionKeyArn: String?
    Notifications: NotificationObjectType?
    LockConfiguration: LockConfigurationType?
    BackupVaultArn: String?
}
class NotificationObjectType {
    BackupVaultEvents: Listing<String>
    SNSTopicArn: String
}
class LockConfigurationType {
    MinRetentionDays: Int
    MaxRetentionDays: Int?
    ChangeableForDays: Int?
}
