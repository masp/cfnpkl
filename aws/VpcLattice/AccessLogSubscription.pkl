/// Enables access logs to be sent to Amazon CloudWatch, Amazon S3, and
/// Amazon Kinesis Data Firehose. The service network owner can use the
/// access logs to audit the services in the network. The service network
/// owner will only see access logs from clients and services that are
/// associated with their service network. Access log entries represent
/// traffic originated from VPCs associated with that network.
extends ".../AWS/Resource.pkl"
Type = "AWS::VpcLattice::AccessLogSubscription"
Properties: Props?
class Props {
    Arn: String(length <= 2048 && length >= 20 && matches(Regex(#"^arn:[a-z0-9\-]+:vpc-lattice:[a-zA-Z0-9\-]+:\d{12}:accesslogsubscription/als-[0-9a-z]{17}$"#)))?
    DestinationArn: String(length <= 2048 && length >= 20 && matches(Regex(#"^arn(:[a-z0-9]+([.-][a-z0-9]+)*){2}(:([a-z0-9]+([.-][a-z0-9]+)*)?){2}:([^/].*)?$"#)))?
    Id: String(length <= 21 && length >= 21 && matches(Regex(#"^als-[0-9a-z]{17}$"#)))?
    ResourceArn: String(length <= 2048 && length >= 20 && matches(Regex(#"^arn(:[a-z0-9]+([.-][a-z0-9]+)*){2}(:([a-z0-9]+([.-][a-z0-9]+)*)?){2}:((servicenetwork/sn)|(service/svc))-[0-9a-z]{17}$"#)))?
    ResourceId: String(length <= 2048 && length >= 20 && matches(Regex(#"^((sn)|(svc))-[0-9a-z]{17}$"#)))?
    ResourceIdentifier: String(length <= 2048 && length >= 20 && matches(Regex(#"^((((sn)|(svc))-[0-9a-z]{17})|(arn(:[a-z0-9]+([.-][a-z0-9]+)*){2}(:([a-z0-9]+([.-][a-z0-9]+)*)?){2}:((servicenetwork/sn)|(service/svc))-[0-9a-z]{17}))$"#)))?
    Tags: Listing<Tag>?
}
class Tag {
    Key: String(length <= 128 && length >= 1)
    Value: String(length <= 256 && length >= 1)
}
