/// Creates or updates the auth policy.
extends ".../AWS/Resource.pkl"
Type = "AWS::VpcLattice::AuthPolicy"
Properties: Props?
class Props {
    ResourceIdentifier: String(length <= 200 && length >= 21 && matches(Regex(#"^((((sn)|(svc))-[0-9a-z]{17})|(arn(:[a-z0-9]+([.-][a-z0-9]+)*){2}(:([a-z0-9]+([.-][a-z0-9]+)*)?){2}:((servicenetwork/sn)|(service/svc))-[0-9a-z]{17}))$"#)))?
    Policy: Mapping?
    State: "ACTIVE" | "INACTIVE" | *Null
}
