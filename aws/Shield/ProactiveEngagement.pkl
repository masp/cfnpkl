/// Authorizes the Shield Response Team (SRT) to use email and phone to
/// notify contacts about escalations to the SRT and to initiate proactive
/// customer support.
extends ".../AWS/Resource.pkl"
Type = "AWS::Shield::ProactiveEngagement"
Properties: Props?
class Props {
    AccountId: String?
    /// If `ENABLED`, the Shield Response Team (SRT) will use email and phone
    /// to notify contacts about escalations to the SRT and to initiate
    /// proactive customer support. If `DISABLED`, the SRT will not
    /// proactively notify contacts about escalations or to initiate proactive
    /// customer support.
    ProactiveEngagementStatus: "ENABLED" | "DISABLED" | *Null
    /// A list of email addresses and phone numbers that the Shield Response
    /// Team (SRT) can use to contact you for escalations to the SRT and to
    /// initiate proactive customer support. To enable proactive engagement,
    /// the contact list must include at least one phone number.
    EmergencyContactList: Listing<EmergencyContact>?
}
/// An emergency contact is used by Shield Response Team (SRT) to contact
/// you for escalations to the SRT and to initiate proactive customer
/// support. An emergency contact requires an email address.
class EmergencyContact {
    /// Additional notes regarding the contact.
    ContactNotes: String(length <= 1024 && length >= 1 && matches(Regex(#"^[\w\s\.\-,:/()+@]*$"#)))?
    /// The email address for the contact.
    EmailAddress: String(length <= 150 && length >= 1 && matches(Regex(#"^\S+@\S+\.\S+$"#)))
    /// The phone number for the contact
    PhoneNumber: String(length <= 16 && length >= 1 && matches(Regex(#"^\+[1-9]\d{1,14}$"#)))?
}
