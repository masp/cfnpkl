/// Resource schema for AWS::ApplicationInsights::Application
extends ".../AWS/Resource.pkl"
Type = "AWS::ApplicationInsights::Application"
Properties: Props?
class Props {
    /// The name of the resource group.
    ResourceGroupName: String(length <= 256 && length >= 1 && matches(Regex(#"[a-zA-Z0-9.-_]*"#)))?
    /// The ARN of the ApplicationInsights application.
    ApplicationARN: String?
    /// Indicates whether Application Insights can listen to CloudWatch events
    /// for the application resources.
    CWEMonitorEnabled: Boolean?
    /// When set to true, creates opsItems for any problems detected on an
    /// application.
    OpsCenterEnabled: Boolean?
    /// The SNS topic provided to Application Insights that is associated to
    /// the created opsItem.
    OpsItemSNSTopicArn: String(length <= 300 && length >= 20 && matches(Regex(#"^arn:aws(-[\w]+)*:[\w\d-]+:([\w\d-]*)?:[\w\d_-]*([:/].+)*$"#)))?
    /// The tags of Application Insights application.
    Tags: Listing<Tag>?
    /// The custom grouped components.
    CustomComponents: Listing<CustomComponent>?
    /// The log pattern sets.
    LogPatternSets: Listing<LogPatternSet>?
    /// If set to true, application will be configured with recommended
    /// monitoring configuration.
    AutoConfigurationEnabled: Boolean?
    /// The monitoring settings of the components.
    ComponentMonitoringSettings: Listing<Mapping | Mapping>?
    /// The grouping type of the application
    GroupingType: "ACCOUNT_BASED" | *Null
}
/// A key-value pair to associate with a resource.
class Tag {
    /// The key name of the tag. You can specify a value that is 1 to 127
    /// Unicode characters in length and cannot be prefixed with aws:. You can
    /// use any of the following characters: the set of Unicode letters,
    /// digits, whitespace, _, ., /, =, +, and -.
    Key: String(length <= 128 && length >= 1)
    /// The value for the tag. You can specify a value that is 1 to 255
    /// Unicode characters in length and cannot be prefixed with aws:. You can
    /// use any of the following characters: the set of Unicode letters,
    /// digits, whitespace, _, ., /, =, +, and -.
    Value: String(length <= 256 && length >= 0)
}
/// The custom grouped component.
class CustomComponent {
    /// The name of the component.
    ComponentName: String(length <= 128 && length >= 1 && matches(Regex(#"^[\d\w\-_.+]*$"#)))
    /// The list of resource ARNs that belong to the component.
    ResourceList: Listing<String(length <= 300 && length >= 20 && matches(Regex(#"^arn:aws(-[\w]+)*:[\w\d-]+:([\w\d-]*)?:[\w\d_-]*([:/].+)*$"#)))>
}
/// The log pattern set.
class LogPatternSet {
    /// The name of the log pattern set.
    PatternSetName: String(length <= 30 && length >= 1 && matches(Regex(#"[a-zA-Z0-9.-_]*"#)))
    /// The log patterns of a set.
    LogPatterns: Listing<LogPattern>
}
/// The log pattern.
class LogPattern {
    /// The name of the log pattern.
    PatternName: String(length <= 50 && length >= 1 && matches(Regex(#"[a-zA-Z0-9.-_]*"#)))
    /// The log pattern.
    Pattern: String(length <= 50 && length >= 1)
    /// Rank of the log pattern.
    Rank: Int
}
