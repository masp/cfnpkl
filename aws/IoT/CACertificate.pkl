/// Registers a CA Certificate in IoT.
extends ".../AWS/Resource.pkl"
Type = "AWS::IoT::CACertificate"
Properties: Props?
class Props {
    CACertificatePem: String(length <= 65536 && length >= 1 && matches(Regex(#"[\s\S]*"#)))?
    /// The private key verification certificate.
    VerificationCertificatePem: String(length <= 65536 && length >= 1 && matches(Regex(#"[\s\S]*"#)))?
    Status: "ACTIVE" | "INACTIVE" | *Null
    CertificateMode: "DEFAULT" | "SNI_ONLY" | *Null
    AutoRegistrationStatus: "ENABLE" | "DISABLE" | *Null
    RemoveAutoRegistration: Boolean?
    RegistrationConfig: RegistrationConfig?
    Id: String?
    Arn: String?
    /// An array of key-value pairs to apply to this resource.
    Tags: Listing<Tag>?
}
class RegistrationConfig {
    TemplateBody: String(length <= 10240 && length >= 0 && matches(Regex(#"[\s\S]*"#)))?
    RoleArn: String(length <= 2048 && length >= 20 && matches(Regex(#"arn:(aws[a-zA-Z-]*)?:iam::\d{12}:role/?[a-zA-Z_0-9+=,.@\-_/]+"#)))?
    TemplateName: String(length <= 36 && length >= 1 && matches(Regex(#"^[0-9A-Za-z_-]+$"#)))?
}
/// A key-value pair to associate with a resource.
class Tag {
    /// The key name of the tag. You can specify a value that is 1 to 127
    /// Unicode characters in length and cannot be prefixed with aws:. You can
    /// use any of the following characters: the set of Unicode letters,
    /// digits, whitespace, _, ., /, =, +, and -.
    Key: String(length <= 127 && length >= 1)
    /// The value for the tag. You can specify a value that is 1 to 255
    /// Unicode characters in length and cannot be prefixed with aws:. You can
    /// use any of the following characters: the set of Unicode letters,
    /// digits, whitespace, _, ., /, =, +, and -.
    Value: String(length <= 255 && length >= 1)
}
