/// Logging Options enable you to configure your IoT V2 logging role and
/// default logging level so that you can monitor progress events logs as
/// it passes from your devices through Iot core service.
extends ".../AWS/Resource.pkl"
Type = "AWS::IoT::Logging"
Properties: Props?
class Props {
    /// Your 12-digit account ID (used as the primary identifier for the
    /// CloudFormation resource).
    AccountId: String(length <= 12 && length >= 12 && matches(Regex(#"^[0-9]{12}$"#)))?
    /// The ARN of the role that allows IoT to write to Cloudwatch logs.
    RoleArn: String(length <= 2048 && length >= 20)?
    /// The log level to use. Valid values are: ERROR, WARN, INFO, DEBUG, or
    /// DISABLED.
    DefaultLogLevel: "ERROR" | "WARN" | "INFO" | "DEBUG" | "DISABLED" | *Null
}
