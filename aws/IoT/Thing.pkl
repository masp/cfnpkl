/// Resource Type definition for AWS::IoT::Thing
extends ".../AWS/Resource.pkl"
Type = "AWS::IoT::Thing"
Properties: Props?
class Props {
    Id: String?
    Arn: String?
    AttributePayload: AttributePayload?
    ThingName: String(length <= 128 && length >= 1 && matches(Regex(#"[a-zA-Z0-9:_-]+"#)))?
}
class AttributePayload {
    Attributes: Mapping?
}
