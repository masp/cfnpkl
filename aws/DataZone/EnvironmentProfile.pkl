/// AWS Datazone Environment Profile is pre-configured set of resources
/// and blueprints that provide reusable templates for creating
/// environments.
extends ".../AWS/Resource.pkl"
Type = "AWS::DataZone::EnvironmentProfile"
Properties: Props?
class Props {
    /// The AWS account in which the Amazon DataZone environment is created.
    AwsAccountId: String(matches(Regex(#"^\d{12}$"#)))?
    /// The AWS region in which this environment profile is created.
    AwsAccountRegion: String(matches(Regex(#"^[a-z]{2}-[a-z]{4,10}-\d$"#)))?
    /// The timestamp of when this environment profile was created.
    CreatedAt: String?
    /// The Amazon DataZone user who created this environment profile.
    CreatedBy: String?
    /// The description of this Amazon DataZone environment profile.
    Description: String(length <= 2048)?
    /// The ID of the Amazon DataZone domain in which this environment profile
    /// is created.
    DomainId: String(matches(Regex(#"^dzd[-_][a-zA-Z0-9_-]{1,36}$"#)))?
    /// The ID of the Amazon DataZone domain in which this environment profile
    /// is created.
    DomainIdentifier: String(matches(Regex(#"^dzd[-_][a-zA-Z0-9_-]{1,36}$"#)))?
    /// The ID of the blueprint with which this environment profile is
    /// created.
    EnvironmentBlueprintId: String(matches(Regex(#"^[a-zA-Z0-9_-]{1,36}$"#)))?
    /// The ID of the blueprint with which this environment profile is
    /// created.
    EnvironmentBlueprintIdentifier: String(matches(Regex(#"^[a-zA-Z0-9_-]{1,36}$"#)))?
    /// The ID of this Amazon DataZone environment profile.
    Id: String(matches(Regex(#"^[a-zA-Z0-9_-]{1,36}$"#)))?
    /// The name of this Amazon DataZone environment profile.
    Name: String(length <= 64 && length >= 1 && matches(Regex(#"^[\w -]+$"#)))?
    /// The identifier of the project in which to create the environment
    /// profile.
    ProjectId: String(matches(Regex(#"^[a-zA-Z0-9_-]{1,36}$"#)))?
    /// The identifier of the project in which to create the environment
    /// profile.
    ProjectIdentifier: String(matches(Regex(#"^[a-zA-Z0-9_-]{1,36}$"#)))?
    /// The timestamp of when this environment profile was updated.
    UpdatedAt: String?
    /// The user parameters of this Amazon DataZone environment profile.
    UserParameters: Listing<EnvironmentParameter>?
}
/// The parameter details of an environment profile.
class EnvironmentParameter {
    /// The name of an environment profile parameter.
    Name: String?
    /// The value of an environment profile parameter.
    Value: String?
}
