/// Resource Type definition for AWS::SES::DedicatedIpPool
extends ".../AWS/Resource.pkl"
Type = "AWS::SES::DedicatedIpPool"
Properties: Props?
class Props {
    /// The name of the dedicated IP pool.
    PoolName: String(matches(Regex(#"^[a-z0-9_-]{0,64}$"#)))?
    /// Specifies whether the dedicated IP pool is managed or not. The default
    /// value is STANDARD.
    ScalingMode: String(matches(Regex(#"^(STANDARD|MANAGED)$"#)))?
}
