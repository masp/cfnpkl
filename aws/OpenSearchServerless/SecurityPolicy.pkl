/// Amazon OpenSearchServerless security policy resource
extends ".../AWS/Resource.pkl"
Type = "AWS::OpenSearchServerless::SecurityPolicy"
Properties: Props?
class Props {
    /// The description of the policy
    Description: String(length <= 1000 && length >= 1)?
    /// The JSON policy document that is the content for the policy
    Policy: String(length <= 20480 && length >= 1 && matches(Regex(#"[\u0009\u000A\u000D\u0020-\u007E\u00A1-\u00FF]+"#)))?
    /// The name of the policy
    Name: String(length <= 32 && length >= 3 && matches(Regex(#"^[a-z][a-z0-9-]{2,31}$"#)))?
    Type: "encryption" | "network" | *Null
}
