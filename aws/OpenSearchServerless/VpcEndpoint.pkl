/// Amazon OpenSearchServerless vpc endpoint resource
extends ".../AWS/Resource.pkl"
Type = "AWS::OpenSearchServerless::VpcEndpoint"
Properties: Props?
class Props {
    /// The identifier of the VPC Endpoint
    Id: String(length <= 255 && length >= 1 && matches(Regex(#"^vpce-[0-9a-z]*$"#)))?
    /// The name of the VPC Endpoint
    Name: String(length <= 32 && length >= 3 && matches(Regex(#"^[a-z][a-z0-9-]{2,31}$"#)))?
    /// The ID of one or more security groups to associate with the endpoint
    /// network interface
    SecurityGroupIds: Listing<String(length <= 128 && length >= 1 && matches(Regex(#"^[\w+\-]+$"#)))>?
    /// The ID of one or more subnets in which to create an endpoint network
    /// interface
    SubnetIds: Listing<String(length <= 32 && length >= 1 && matches(Regex(#"^subnet-([0-9a-f]{8}|[0-9a-f]{17})$"#)))>?
    /// The ID of the VPC in which the endpoint will be used.
    VpcId: String(length <= 255 && length >= 1 && matches(Regex(#"^vpc-[0-9a-z]*$"#)))?
}
