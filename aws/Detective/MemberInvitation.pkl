/// Resource schema for AWS::Detective::MemberInvitation
extends ".../AWS/Resource.pkl"
Type = "AWS::Detective::MemberInvitation"
Properties: Props?
class Props {
    /// The ARN of the graph to which the member account will be invited
    GraphArn: String(matches(Regex(#"arn:aws(-[\w]+)*:detective:(([a-z]+-)+[0-9]+):[0-9]{12}:graph:[0-9a-f]{32}"#)))?
    /// The AWS account ID to be invited to join the graph as a member
    MemberId: String(matches(Regex(#"[0-9]{12}"#)))?
    /// The root email address for the account to be invited, for validation.
    /// Updating this field has no effect.
    MemberEmailAddress: String(matches(Regex(#".*@.*"#)))?
    /// When set to true, invitation emails are not sent to the member
    /// accounts. Member accounts must still accept the invitation before they
    /// are added to the behavior graph. Updating this field has no effect.
    DisableEmailNotification: Boolean?
    /// A message to be included in the email invitation sent to the invited
    /// account. Updating this field has no effect.
    Message: String(length <= 1000 && length >= 1)?
}
