/// A resource schema for a Variable in Amazon Fraud Detector.
extends ".../AWS/Resource.pkl"
Type = "AWS::FraudDetector::Variable"
Properties: Props?
class Props {
    /// The name of the variable.
    Name: String(matches(Regex(#"^[a-z_][a-z0-9_]{0,99}?$"#)))?
    /// The source of the data.
    DataSource: "EVENT" | "EXTERNAL_MODEL_SCORE" | *Null
    /// The data type.
    DataType: "STRING" | "INTEGER" | "FLOAT" | "BOOLEAN" | *Null
    /// The default value for the variable when no value is received.
    DefaultValue: String?
    /// The description.
    Description: String(length <= 256 && length >= 1)?
    /// Tags associated with this variable.
    Tags: Listing<Tag>?
    /// The variable type. For more information see
    /// https://docs.aws.amazon.com/frauddetector/latest/ug/create-a-
    /// variable.html#variable-types
    VariableType: "AUTH_CODE" | "AVS" | "BILLING_ADDRESS_L1" | "BILLING_ADDRESS_L2" | "BILLING_CITY" | "BILLING_COUNTRY" | "BILLING_NAME" | "BILLING_PHONE" | "BILLING_STATE" | "BILLING_ZIP" | "CARD_BIN" | "CATEGORICAL" | "CURRENCY_CODE" | "EMAIL_ADDRESS" | "FINGERPRINT" | "FRAUD_LABEL" | "FREE_FORM_TEXT" | "IP_ADDRESS" | "NUMERIC" | "ORDER_ID" | "PAYMENT_TYPE" | "PHONE_NUMBER" | "PRICE" | "PRODUCT_CATEGORY" | "SHIPPING_ADDRESS_L1" | "SHIPPING_ADDRESS_L2" | "SHIPPING_CITY" | "SHIPPING_COUNTRY" | "SHIPPING_NAME" | "SHIPPING_PHONE" | "SHIPPING_STATE" | "SHIPPING_ZIP" | "USERAGENT" | *Null
    /// The ARN of the variable.
    Arn: String?
    /// The time when the variable was created.
    CreatedTime: String?
    /// The time when the variable was last updated.
    LastUpdatedTime: String?
}
class Tag {
    Key: String(length <= 128 && length >= 1)
    Value: String(length <= 256 && length >= 0)
}
