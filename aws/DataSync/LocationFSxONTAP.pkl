/// Resource schema for AWS::DataSync::LocationFSxONTAP.
extends ".../AWS/Resource.pkl"
Type = "AWS::DataSync::LocationFSxONTAP"
Properties: Props?
class Props {
    /// The Amazon Resource Name (ARN) for the FSx ONTAP SVM.
    StorageVirtualMachineArn: String(length <= 162 && matches(Regex(#"^arn:(aws|aws-cn|aws-us-gov|aws-iso|aws-iso-b):fsx:[a-z\-0-9]+:[0-9]{12}:storage-virtual-machine/fs-[0-9a-f]+/svm-[0-9a-f]{17,}$"#)))?
    /// The Amazon Resource Name (ARN) for the FSx ONAP file system.
    FsxFilesystemArn: String(length <= 128 && matches(Regex(#"^arn:(aws|aws-cn|aws-us-gov|aws-iso|aws-iso-b):fsx:[a-z\-0-9]+:[0-9]{12}:file-system/fs-[0-9a-f]+$"#)))?
    /// The ARNs of the security groups that are to use to configure the FSx
    /// ONTAP file system.
    SecurityGroupArns: Listing<String(length <= 128 && matches(Regex(#"^arn:(aws|aws-cn|aws-us-gov|aws-iso|aws-iso-b):ec2:[a-z\-0-9]*:[0-9]{12}:security-group/sg-[a-f0-9]+$"#)))>?
    Protocol: Protocol?
    /// A subdirectory in the location's path.
    Subdirectory: String(length <= 4096 && matches(Regex(#"^[a-zA-Z0-9_\-\+\./\(\)\$\p{Zs}]+$"#)))?
    /// An array of key-value pairs to apply to this resource.
    Tags: Listing<Tag>?
    /// The Amazon Resource Name (ARN) of the Amazon FSx ONTAP file system
    /// location that is created.
    LocationArn: String(length <= 128 && matches(Regex(#"^arn:(aws|aws-cn|aws-us-gov|aws-iso|aws-iso-b):datasync:[a-z\-0-9]+:[0-9]{12}:location/loc-[0-9a-z]{17}$"#)))?
    /// The URL of the FSx ONTAP file system that was described.
    LocationUri: String(length <= 4360 && matches(Regex(#"^(efs|nfs|s3|smb|hdfs|fsx[a-z0-9-]+)://[a-zA-Z0-9.:/\-]+$"#)))?
}
/// Configuration settings for NFS or SMB protocol.
class Protocol {
    NFS: NFS?
    SMB: SMB?
}
/// NFS protocol configuration for FSx ONTAP file system.
class NFS {
    MountOptions: NfsMountOptions
}
/// The NFS mount options that DataSync can use to mount your NFS share.
class NfsMountOptions {
    /// The specific NFS version that you want DataSync to use to mount your
    /// NFS share.
    Version: "AUTOMATIC" | "NFS3" | "NFS4_0" | "NFS4_1" | *Null
}
/// SMB protocol configuration for FSx ONTAP file system.
class SMB {
    MountOptions: SmbMountOptions
    /// The name of the Windows domain that the SMB server belongs to.
    Domain: String(length <= 253 && matches(Regex(#"^([A-Za-z0-9]+[A-Za-z0-9-.]*)*[A-Za-z0-9-]*[A-Za-z0-9]$"#)))?
    /// The password of the user who can mount the share and has the
    /// permissions to access files and folders in the SMB share.
    Password: String(length <= 104 && matches(Regex(#"^.{0,104}$"#)))
    /// The user who can mount the share, has the permissions to access files
    /// and folders in the SMB share.
    User: String(length <= 104 && matches(Regex(#"^[^\x5B\x5D\\/:;|=,+*?]{1,104}$"#)))
}
/// The mount options used by DataSync to access the SMB server.
class SmbMountOptions {
    /// The specific SMB version that you want DataSync to use to mount your
    /// SMB share.
    Version: "AUTOMATIC" | "SMB2" | "SMB3" | *Null
}
/// A key-value pair to associate with a resource.
class Tag {
    /// The key for an AWS resource tag.
    Key: String(length <= 256 && length >= 1 && matches(Regex(#"^[a-zA-Z0-9\s+=._:/-]+$"#)))
    /// The value for an AWS resource tag.
    Value: String(length <= 256 && length >= 1 && matches(Regex(#"^[a-zA-Z0-9\s+=._:@/-]+$"#)))
}
