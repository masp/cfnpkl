///  A delivery source is an AWS resource that sends logs to an AWS
/// destination. The destination can be CloudWatch Logs, Amazon S3, or
/// Kinesis Data Firehose.  Only some AWS services support being
/// configured as a delivery source. These services are listed as
/// Supported [V2 Permissions] in the table at [Enabling logging from AWS 
/// services](https://docs.aws.amazon.com/AmazonCloudWatch/latest/logs/AWS
/// -logs-and-resource-policy.html).
extends ".../AWS/Resource.pkl"
Type = "AWS::Logs::DeliverySource"
Properties: Props?
class Props {
    /// The unique name of the Log source.
    Name: String(length <= 60 && length >= 1 && matches(Regex(#"[\w-]*$"#)))?
    /// The Amazon Resource Name (ARN) that uniquely identifies this delivery
    /// source.
    Arn: String(length <= 2048 && length >= 16 && matches(Regex(#"^arn:(aws[a-zA-Z-]*)?:[A-Za-z0-9][A-Za-z0-9_/.-]{0,62}:[A-Za-z0-9_/.-]{0,63}:[A-Za-z0-9_/.-]{0,63}:[A-Za-z0-9][A-Za-z0-9:_/+=,@.-]{0,1023}$"#)))?
    /// This array contains the ARN of the AWS resource that sends logs and is
    /// represented by this delivery source. Currently, only one ARN can be in
    /// the array.
    ResourceArns: Listing<String(length <= 2048 && length >= 16 && matches(Regex(#"^arn:(aws[a-zA-Z-]*)?:[A-Za-z0-9][A-Za-z0-9_/.-]{0,62}:[A-Za-z0-9_/.-]{0,63}:[A-Za-z0-9_/.-]{0,63}:[A-Za-z0-9][A-Za-z0-9:_/+=,@.-]{0,1023}$"#)))>?
    /// The ARN of the resource that will be sending the logs.
    ResourceArn: String(length <= 2048 && length >= 16 && matches(Regex(#"^arn:(aws[a-zA-Z-]*)?:[A-Za-z0-9][A-Za-z0-9_/.-]{0,62}:[A-Za-z0-9_/.-]{0,63}:[A-Za-z0-9_/.-]{0,63}:[A-Za-z0-9][A-Za-z0-9:_/+=,@.-]{0,1023}$"#)))?
    /// The AWS service that is sending logs.
    Service: String(length <= 255 && length >= 1 && matches(Regex(#"[\w-]*$"#)))?
    /// The type of logs being delivered. Only mandatory when the resourceArn
    /// could match more than one. In such a case, the error message will
    /// contain all the possible options.
    LogType: String(length <= 255 && length >= 1 && matches(Regex(#"[\w-]*$"#)))?
    /// The tags that have been assigned to this delivery source.
    Tags: Listing<Tag>?
}
/// A key-value pair to associate with a resource.
class Tag {
    /// The key name of the tag. You can specify a value that is 1 to 128
    /// Unicode
    Key: String(length <= 128 && length >= 1)
    /// The value for the tag. You can specify a value that is 0 to 256
    /// Unicode
    Value: String(length <= 256 && length >= 0)
}
