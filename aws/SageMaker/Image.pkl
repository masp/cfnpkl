/// Resource Type definition for AWS::SageMaker::Image
extends ".../AWS/Resource.pkl"
Type = "AWS::SageMaker::Image"
Properties: Props?
class Props {
    ImageName: String(length <= 63 && length >= 1 && matches(Regex(#"^[a-zA-Z0-9]([-.]?[a-zA-Z0-9])*$"#)))?
    ImageArn: String(length <= 256 && length >= 1 && matches(Regex(#"^arn:aws(-[\w]+)*:sagemaker:[a-z0-9\-]*:[0-9]{12}:image\/[a-zA-Z0-9]([-.]?[a-zA-Z0-9])*$"#)))?
    ImageRoleArn: String(length <= 256 && length >= 1 && matches(Regex(#"^arn:aws(-[\w]+)*:iam::[0-9]{12}:role/.*$"#)))?
    ImageDisplayName: String(length <= 128 && length >= 1 && matches(Regex(#"^[A-Za-z0-9 -_]+$"#)))?
    ImageDescription: String(length <= 512 && length >= 1 && matches(Regex(#".+"#)))?
    /// An array of key-value pairs to apply to this resource.
    Tags: Listing<Tag>?
}
/// A key-value pair to associate with a resource.
class Tag {
    /// The key name of the tag. You can specify a value that is 1 to 127
    /// Unicode characters in length and cannot be prefixed with aws:. You can
    /// use any of the following characters: the set of Unicode letters,
    /// digits, whitespace, _, ., /, =, +, and -.
    Key: String(length <= 128 && length >= 1)
    /// The value for the tag. You can specify a value that is 1 to 255
    /// Unicode characters in length and cannot be prefixed with aws:. You can
    /// use any of the following characters: the set of Unicode letters,
    /// digits, whitespace, _, ., /, =, +, and -.
    Value: String(length <= 256)
}
