/// AWS::SimSpaceWeaver::Simulation resource creates an AWS Simulation.
extends ".../AWS/Resource.pkl"
Type = "AWS::SimSpaceWeaver::Simulation"
Properties: Props?
class Props {
    /// The name of the simulation.
    Name: String(length <= 2048 && length >= 1 && matches(Regex(#"[a-zA-Z0-9_\-]{1,2048}$"#)))?
    /// Role ARN.
    RoleArn: String?
    SchemaS3Location: S3Location?
    /// Json object with all simulation details
    DescribePayload: String?
    /// The maximum running time of the simulation.
    MaximumDuration: String(length <= 6 && length >= 2)?
    SnapshotS3Location: S3Location?
}
class S3Location {
    /// The Schema S3 bucket name.
    BucketName: String(length <= 63 && length >= 3 && matches(Regex(#"[a-zA-Z0-9_\-]{3,63}$"#)))
    /// This is the schema S3 object key, which includes the full path of
    /// "folders" from the bucket root to the schema.
    ObjectKey: String(length <= 255 && length >= 3)
}
