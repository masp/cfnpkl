/// Schema for Lambda LayerVersionPermission
extends ".../AWS/Resource.pkl"
Type = "AWS::Lambda::LayerVersionPermission"
Properties: Props?
class Props {
    /// ID generated by service
    Id: String?
    /// The API action that grants access to the layer.
    Action: String?
    /// The name or Amazon Resource Name (ARN) of the layer.
    LayerVersionArn: String?
    /// With the principal set to *, grant permission to all accounts in the
    /// specified organization.
    OrganizationId: String?
    /// An account ID, or * to grant layer usage permission to all accounts in
    /// an organization, or all AWS accounts (if organizationId is not
    /// specified).
    Principal: String?
}
