/// The AWS::CloudFormation::Stack resource nests a stack as a resource in
/// a top-level template.
extends ".../AWS/Resource.pkl"
Type = "AWS::CloudFormation::Stack"
Properties: Props?
class Props {
    Capabilities: Listing<"CAPABILITY_IAM" | "CAPABILITY_NAMED_IAM" | "CAPABILITY_AUTO_EXPAND">?
    RoleARN: String?
    Outputs: Listing<Output>?
    Description: String(length <= 1024 && length >= 1)?
    DisableRollback: Boolean?
    EnableTerminationProtection: Boolean?
    NotificationARNs: Listing<String>?
    Parameters: Mapping?
    ParentId: String?
    RootId: String?
    ChangeSetId: String?
    StackName: String?
    StackId: String?
    StackPolicyBody: Mapping?
    StackPolicyURL: String?
    StackStatus: "CREATE_IN_PROGRESS" | "CREATE_FAILED" | "CREATE_COMPLETE" | "ROLLBACK_IN_PROGRESS" | "ROLLBACK_FAILED" | "ROLLBACK_COMPLETE" | "DELETE_IN_PROGRESS" | "DELETE_FAILED" | "DELETE_COMPLETE" | "UPDATE_IN_PROGRESS" | "UPDATE_COMPLETE_CLEANUP_IN_PROGRESS" | "UPDATE_COMPLETE" | "UPDATE_FAILED" | "UPDATE_ROLLBACK_IN_PROGRESS" | "UPDATE_ROLLBACK_FAILED" | "UPDATE_ROLLBACK_COMPLETE_CLEANUP_IN_PROGRESS" | "UPDATE_ROLLBACK_COMPLETE" | "REVIEW_IN_PROGRESS" | "IMPORT_IN_PROGRESS" | "IMPORT_COMPLETE" | "IMPORT_ROLLBACK_IN_PROGRESS" | "IMPORT_ROLLBACK_FAILED" | "IMPORT_ROLLBACK_COMPLETE" | *Null
    StackStatusReason: String?
    Tags: Listing<Tag>?
    TemplateBody: Mapping?
    TemplateURL: String(length <= 1024 && length >= 1)?
    TimeoutInMinutes: Int?
    LastUpdateTime: String?
    CreationTime: String?
}
class Output {
    Description: String?
    ExportName: String?
    OutputKey: String?
    OutputValue: String?
}
class Tag {
    Key: String
    Value: String
}
