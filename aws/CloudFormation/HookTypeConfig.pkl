/// Specifies the configuration data for a registered hook in
/// CloudFormation Registry.
extends ".../AWS/Resource.pkl"
Type = "AWS::CloudFormation::HookTypeConfig"
Properties: Props?
class Props {
    /// The Amazon Resource Name (ARN) of the type without version number.
    TypeArn: String(matches(Regex(#"^arn:aws[A-Za-z0-9-]{0,64}:cloudformation:[A-Za-z0-9-]{1,64}:([0-9]{12})?:type/hook/.+$"#)))?
    /// The name of the type being registered.  We recommend that type names
    /// adhere to the following pattern:
    /// company_or_organization::service::type.
    TypeName: String(matches(Regex(#"^[A-Za-z0-9]{2,64}::[A-Za-z0-9]{2,64}::[A-Za-z0-9]{2,64}$"#)))?
    /// The Amazon Resource Name (ARN) for the configuration data, in this
    /// account and region.
    ConfigurationArn: String(matches(Regex(#"^arn:aws[A-Za-z0-9-]{0,64}:cloudformation:[A-Za-z0-9-]{1,64}:([0-9]{12})?:type(-configuration)?/hook/.+$"#)))?
    /// The configuration data for the extension, in this account and region.
    Configuration: String(matches(Regex(#"[\s\S]+"#)))?
    /// An alias by which to refer to this extension configuration data.
    ConfigurationAlias: "default"(matches(Regex(#"^[a-zA-Z0-9]{1,256}$"#))) | *Null
}
