/// Set a version as default version for a hook in CloudFormation
/// Registry.
extends ".../AWS/Resource.pkl"
Type = "AWS::CloudFormation::HookDefaultVersion"
Properties: Props?
class Props {
    /// The Amazon Resource Name (ARN) of the type version.
    TypeVersionArn: String(matches(Regex(#"^arn:aws[A-Za-z0-9-]{0,64}:cloudformation:[A-Za-z0-9-]{1,64}:([0-9]{12})?:type/hook/.+$"#)))?
    /// The name of the type being registered.  We recommend that type names
    /// adhere to the following pattern:
    /// company_or_organization::service::type.
    TypeName: String(matches(Regex(#"^[A-Za-z0-9]{2,64}::[A-Za-z0-9]{2,64}::[A-Za-z0-9]{2,64}$"#)))?
    /// The Amazon Resource Name (ARN) of the type. This is used to uniquely
    /// identify a HookDefaultVersion
    Arn: String(matches(Regex(#"^arn:aws[A-Za-z0-9-]{0,64}:cloudformation:[A-Za-z0-9-]{1,64}:([0-9]{12})?:type/hook/.+$"#)))?
    /// The ID of an existing version of the hook to set as the default.
    VersionId: String(matches(Regex(#"^[A-Za-z0-9-]{1,128}$"#)))?
}
