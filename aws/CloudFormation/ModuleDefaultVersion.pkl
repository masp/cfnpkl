/// A module that has been registered in the CloudFormation registry as
/// the default version
extends ".../AWS/Resource.pkl"
Type = "AWS::CloudFormation::ModuleDefaultVersion"
Properties: Props?
class Props {
    /// The Amazon Resource Name (ARN) of the module version to set as the
    /// default version.
    Arn: String(matches(Regex(#"^arn:aws[A-Za-z0-9-]{0,64}:cloudformation:[A-Za-z0-9-]{1,64}:([0-9]{12})?:type/module/.+/[0-9]{8}$"#)))?
    /// The name of a module existing in the registry.
    ModuleName: String(matches(Regex(#"^[A-Za-z0-9]{2,64}::[A-Za-z0-9]{2,64}::[A-Za-z0-9]{2,64}::MODULE"#)))?
    /// The ID of an existing version of the named module to set as the
    /// default.
    VersionId: String(matches(Regex(#"^[0-9]{8}$"#)))?
}
